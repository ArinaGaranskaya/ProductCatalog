@model ProductCatalog.Models.Categorys
@{
    ViewBag.Title = "Редактирование категории";
}
<form asp-action="EditCat" asp-controller="home" asp-route-id="@Model.id">
    <a>Редактирование категории продуктов </a><a></a>
    <div class="form-group">
        <label asp-for="Category" class="control-label">Название категории</label>
        <input type="text" asp-for="Category" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>
